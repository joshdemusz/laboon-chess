<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.*?>

<AnchorPane fx:controller="NewGameController" prefHeight="148.0" prefWidth="394.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="174.0" prefWidth="394.0">
         <children>
            <Pane prefHeight="41.0" prefWidth="394.0">
               <children>
                  <Label layoutX="19.0" layoutY="-1.0" prefHeight="39.0" prefWidth="103.0" text="Color:" />
                  <ChoiceBox fx:id="chooseColor" value="Black" layoutX="122.0" layoutY="5.0" prefWidth="150.0">
                     <items>
                        <FXCollections fx:factory="observableArrayList">
                           <String fx:value="Black" />
                           <String fx:value="White" />
                        </FXCollections>
                     </items>
                  </ChoiceBox>
               </children>
            </Pane>
            <Pane prefHeight="44.0" prefWidth="394.0">
               <children>
                  <ChoiceBox fx:id="chooseDifficulty" value="Easy" layoutX="122.0" layoutY="9.0" prefWidth="150.0">
                     <items>
                        <FXCollections fx:factory="observableArrayList">
                           <String fx:value="Easy" />
                        </FXCollections>
                     </items>
                  </ChoiceBox>
                  <Label layoutX="14.0" layoutY="3.0" prefHeight="39.0" prefWidth="103.0" text=" Difficulty :" />
               </children>
            </Pane>
            <Pane prefHeight="54.0" prefWidth="394.0">
               <children>
                  <Label layoutX="17.0" layoutY="13.0" prefHeight="17.0" prefWidth="218.0" text="Going First: " />
               </children>
            </Pane>
            <Pane prefHeight="46.0" prefWidth="394.0">
               <children>
                  <HBox layoutX="188.0" prefHeight="47.0" prefWidth="200.0">
                     <children>
                        <Pane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Button fx:id="submitButton" onAction="#submitButtonAction" layoutX="50.0" layoutY="10.0" mnemonicParsing="false" text="Start" />
                           </children>
                        </Pane>
                        <Pane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Button fx:id="cancelButton" onAction="#cancelButtonAction" cancelButton="true" defaultButton="true" layoutX="39.0" layoutY="10.0" mnemonicParsing="false" text="Cancel" />
                           </children>
                        </Pane>
                     </children>
                  </HBox>
               </children>
            </Pane>
         </children>
      </VBox>
   </children>

</AnchorPane>
